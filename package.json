{
    "name": "flowmaker",
    "displayName": "flowmaker",
    "description": "Vs code extension for live js code visualization'",
    "version": "0.0.1",
    "publisher": "speks",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#0D47A1",
        "theme": "dark"
    },
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.onBrowser",
                "title": "Flowmaker on browser"
            },
            {
                "command": "extension.onEditor",
                "title": "Flowmaker on Editor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/aryaminus/flowmaker/issues",
        "email": "sunim.54@gmail.com"
    },
        "dependencies": {
        "express": "^4.16.2",
        "js2flowchart": "^1.1.3",
        "socket.io": "^2.0.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aryaminus/flowmaker.git"
    },
    "homepage": "https://github.com/aryaminus/flowmaker/blob/master/README.md"

}
